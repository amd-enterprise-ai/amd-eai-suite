<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd
                   http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

<changeSet author="tomas" id="add-model-metadata-table">
    <createTable tableName="model_metadata">
        <column name="canonical_name" type="character varying">
            <constraints nullable="false" unique="true" uniqueConstraintName="model_metadata_canonical_name_key"/>
        </column>
        <column name="display_name" type="character varying">
            <constraints nullable="false"/>
        </column>
        <column name="description" type="character varying"/>
        <column name="compatible_inference_engines" type="character varying[]">
            <constraints nullable="false"/>
        </column>
        <column name="capabilities" type="character varying[]"/>
        <column name="model_url" type="character varying"/>
        <column name="id" type="uuid" defaultValueComputed="gen_random_uuid()">
            <constraints primaryKey="true" primaryKeyName="model_metadata_pkey"/>
        </column>
        <column name="created_at" type="timestamp with time zone">
            <constraints nullable="false"/>
        </column>
        <column name="updated_at" type="timestamp with time zone">
            <constraints nullable="false"/>
        </column>
        <column name="created_by" type="character varying"/>
        <column name="updated_by" type="character varying"/>
    </createTable>

    <createIndex indexName="idx_model_metadata_canonical_name" tableName="model_metadata">
        <column name="canonical_name"/>
    </createIndex>

